@model IEnumerable<EresData.GradeValue>

@{
    ViewBag.Title = "OcenyStudenta";
}

<h2>OcenyStudenta</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GradeID)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Grade.Name)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.ActionLink("Edytuj", "EditGV", new { id=item.GradeValueID }) |

        </td>
    </tr>
}

</table>
