@model IEnumerable<EresData.Realisation>

@{
    ViewBag.Title = "Index";
}

<h2>Realizacje</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SubjectID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SemesterID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Subject.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semester.Name)
        </td>
        <td>
            @Html.ActionLink("Przejdz do studentow", "Details", new { id=item.RealisationID })
        </td>
    </tr>
}

   

</table>
 @Html.ActionLink("Powrot","Index", "Home") <br />
